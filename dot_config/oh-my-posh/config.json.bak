{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "version": 1,
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "path",
          "style": "plain",
          "properties": {
            "style": "folder",
            "max_depth": 2,
            "exclude_folders": [
              "node_modules",
              ".git"
            ]
          },
          "foreground": "#4EC9B0",
          "background": "#1E1E1E"
        },
        {
          "type": "git",
          "style": "plain",
          "properties": {
            "display_stash_count": true,
            "display_upstream_icon": true,
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream": true
          },
          "foreground": "#FF8C00",
          "background": "#1E1E1E"
        },
        {
          "type": "node",
          "style": "plain",
          "properties": {
            "fetch_version": true,
            "display_mode": "files"
          },
          "foreground": "#4EC9B0",
          "background": "#1E1E1E"
        },
        {
          "type": "exit",
          "style": "plain",
          "properties": {
            "display_exit_code": true,
            "error_color": "#F44747",
            "success_color": "#4EC9B0"
          },
          "background": "#1E1E1E"
        },
        {
          "type": "text",
          "style": "plain",
          "properties": {
            "text": " "
          }
        },
        {
          "type": "text",
          "style": "plain",
          "properties": {
            "text": "‚ùØ"
          },
          "foreground": "#4EC9B0",
          "background": "#1E1E1E"
        }
      ]
    }
  ]
}
